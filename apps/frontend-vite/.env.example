# LifeBit Frontend 환경변수 설정

# 소셜 로그인 설정
VITE_GOOGLE_CLIENT_ID=your-key-here
VITE_KAKAO_CLIENT_ID=your-key-here

# API 서버 설정 (로컬 개발용 기본값)
VITE_CORE_API_URL=http://localhost:8080
VITE_AI_API_URL=http://localhost:8001

# 프로덕션 환경에서는 Nginx 프록시를 통해 자동으로 라우팅되므로
# 이 설정들은 개발 환경에서만 사용됩니다.

# 환경 설정
NODE_ENV=development

# 인증 설정
VITE_JWT_SECRET_KEY=your-key-here

# OpenAI API 설정 (프론트엔드 직접 호출용)
VITE_OPENAI_API_KEY=your-key-here
